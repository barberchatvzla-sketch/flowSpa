{
  "functions": {
    "api/preview.js": {
      "maxDuration": 10,
      "memory": 1024
    }
  },
  "headers": [
    {
      "source": "/.well-known/assetlinks.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/reserva/:ref",
      "destination": "/api/preview?ref=:ref"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}
